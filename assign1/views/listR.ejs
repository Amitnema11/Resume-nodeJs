<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>

   <div class="container text-center ">
    <h1 class="alert alert-primary" >List of Resume</h1>
<table class="table">
    <thead>
      <tr>
<th>No</th>
<th>Name</th>
<th>Email</th>
<th>Action</th>
</tr>
    </thead>
<tbody>
  <% var i = 1 %>
<% tasks.forEach(function(task){%>
  <tr>
<td><%= i++ %> </td>
 <td> <%= task.name %> </td>
<td> <%= task.email %> </td>

<td><a href="/view/<%= task._id%>"><button class="btn btn-primary">View</button></a></td>
<td><a href="/delete/<%=task._id%>"><button class="btn btn-primary">Delete</button></a></td>





<!-- Contact: <%= task.contact %>
<img width="200" src="  <%= task.photo %>
" alt=""><br>
Skill: <%= task.skill %> <br>
Experience: <%= task.experience %> <br>
Projects: <%= task.projects %> <br>
Education: <%= task.education %>  -->


</tr>
<% }) %>

</tbody>
</table>
<a href="/"><button class="btn btn-primary">Home</button></a>
</div>

  </body>
</html>
